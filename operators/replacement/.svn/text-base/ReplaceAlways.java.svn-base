 * Replacement operator. Replaces the current individual in the population allways
 * 
 */

package operators.replacement;

import jcell.*;

public class ReplaceAlways implements Operator
{
   //Parametro array de individuos, devuelve individuo
   public Object execute(Object o)
   {
      Individual iv[] = (Individual[])o;
      
      return iv[0]; //Nuevo individuo en posicion 0
   }
}
